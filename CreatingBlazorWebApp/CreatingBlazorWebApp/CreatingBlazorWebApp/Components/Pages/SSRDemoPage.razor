@page "/ssrdemo"
<h3>SSR Demo Page</h3>

<p>
    <button class="btn btn-primary" type="button" @onclick="SubmitClick">Submit</button>
</p>
<p>
    @message
</p>

@code {
    private string message = "Hello from SSR Demo Page";

    private void SubmitClick()
    {
        message = "Button clicked"; // This will not be rendered
    }
}